{
  "extensionName": { "message": "tweetbot.sh remote controller" },
  "extensionDescription": { "message": "Provide context menu commands to share tweets by tweetbot.sh" },

  "menu_fav_label": { "message": "Favorite this" },
  "menu_unfav_label": { "message": "Unfavorite this" },
  "menu_rt_label": { "message": "RT this" },
  "menu_unrt_label": { "message": "Cancel RT of this" },
  "menu_rt_now_label": { "message": "RT this immediately" },
  "menu_fav_and_rt_label": { "message": "Favorite and RT this" },
  "menu_fav_and_rt_now_label": { "message": "Favorite and RT this immediately" },
  "menu_follow_label": { "message": "Follow" },
  "menu_unfollow_label": { "message": "Unfollow" },
  "menu_test_label": { "message": "Test" },

  "notConfiguredError_title": { "message": "Not configured!" },
  "notConfiguredError_message": { "message": "Please set path to the tweet.sh and the screen name of the account to be controlled." },

  "notTweetError_title": { "message": "Not a tweet!" },
  "notTweetError_message": { "message": "$URL$ is not a tweet.",
    "placeholders": {
      "url": {
        "content": "$1",
        "example": "http://..."
      }
    }
  },

  "unknownCommand_title": { "message": "Unknown command" },
  "unknownCommand_message": { "message": "Failed to process unknown command: $COMMAND$",
    "placeholders": {
      "command": {
        "content": "$1",
        "example": "abc"
      }
    }
  },

  "onResponse_title": { "message": "Operation succeeded." },
  "onResponse_message": { "message": "Command successfully sent: $COMMAND$",
    "placeholders": {
      "command": {
        "content": "$1",
        "example": "fav xxx"
      }
    }
  },

  "onError_title": { "message": "Operation failed." },
  "onError_message": { "message": "Command: $COMMAND$\nError: $ERROR$",
    "placeholders": {
      "command": {
        "content": "$1",
        "example": "fav xxx"
      },
      "error": {
        "content": "$2",
        "example": "???"
      }
    }
  },

  "config_tweetsh_path_label_before": { "message": "Full path to the tweet.sh:" },
  "config_tweetsh_path_label_after": { "message": "" },
  "config_target_account_label_before": { "message": "Screnen name of the bot account to be controlled:" },
  "config_target_account_label_after": { "message": "" },
  "config_debug_label": { "message": "Print debug log" }
}
